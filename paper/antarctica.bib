  author = {Piscov{\'a}, Veronika and {\v S}ev{\v c}{\'i}k, Michal and Sedl{\'a}k, Andrej and Hre{\v s}ko, Juraj and Petrovi{\v c}, Franti{\v s}ek and Slobodov{\'a}, Ter{\'e}zia},
